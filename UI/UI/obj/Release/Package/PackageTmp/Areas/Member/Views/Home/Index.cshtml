<div id="fh5co-course">
    <div class="container">
        <div class="row animate-box">
            <div class="col-md-6 col-md-offset-3 text-center fh5co-heading">
                <h2><a href="/Member/Education/Index" style="color:black;">EĞİTİMLER</a></h2>
                @*<p>Dignissimos asperiores vitae velit veniam totam fuga molestias accusamus alias autem provident. Odit ab aliquam dolor eius.</p>*@
            </div>
        </div>
        <div class="row">
            @foreach (var item in educations)
            {
                if (eduCount < 4)
                {
                    <div class="col-md-6 animate-box">
                        <div class="course">
                            <a href="#" class="course-img" style="background-image: url(@item.ImagePath);">
                            </a>
                            <div class="desc">
                                <h3><a href="#">@item.Name</a></h3>
                                <p>@item.Note</p>
                                <span><a href="/Member/Education/LessonList?id=@item.ID" class="btn btn-primary btn-sm btn-course">DERSLERİ GÖR</a></span>
                            </div>
                        </div>
                    </div>
                    eduCount++;
                }
            }
        </div>
    </div>
</div>

